{
  "manifest_version": 2,
  "name": "C19-Search!",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "main.js"],
      "css": ["styles.css"],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
